{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": [
      "bin",
      "build/**",
      "dist/**",
      "config/**",
      "coverage/**",
      "node_modules/**",
      "**/*.d.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-console": [true, "log"],
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}
