{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "modules": "./src/modules",
          "template-tags": "./src/template-tags"
        }
      }
    ],
    "babel-plugin-styled-components",
    "@babel/plugin-proposal-class-properties"
  ]
}